    "bump": "bumpme",
    "gen-env": "sed 's/=.*/=/' .env.example > .env",
    "setup": "./setup.sh"
  },
  "author": "Upsales dev",
  "license": "ISC",
  "dependencies": {
    "@upsales/node-upsales": "^1.1.2",
    "@upsales/node-upsales-promise": "^1.4.6",
    "aws-serverless-express": "^3.3.8",
    "axios": "^0.20.0",
    "axios-retry": "^3.1.9",
    "bumpme": "^1.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jest": "^26.5.2",
    "serialize-error": "^7.0.1",
    "winston": "^3.3.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,}",
      "!**/node_modules/**"
    ],
    "testPathIgnorePatterns": [
      "testObjects.js"
    ]
  },
  "devDependencies": {
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "node-dev": "^5.2.0",
    "prettier": "^2.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
